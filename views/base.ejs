    <link rel="stylesheet" href="stylesheets/common.css?ver=<%=site.cssver%>">
    <%-defineContent('header')%>

</head>
<body>


        <!-- Left Panel -->

    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">

            <div class="navbar-header">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-menu" aria-controls="main-menu" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand" href="./"><img src="images/logo.png" alt="Logo"></a>
                <a class="navbar-brand hidden" href="./"><img src="images/logo2.png" alt="Logo"></a>
            </div>

            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <!--
                    <li class="active">
                        <a href="/attendances"> <i class="menu-icon fa fa-dashboard"></i>출퇴근 </a>
                    </li>
                    -->
                    <h3 class="menu-title">사내업무</h3><!-- /.menu-title -->
                    <% if(path1 == "attendances") { %>
                    <li class="menu-item-has-children active dropdown show">
                    <% } else{ %>
                    <li class="menu-item-has-children dropdown">
                    <% } %>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-industry"></i>근태관리</a>
                        <ul class="sub-menu children activedropdown-menu show">
                            <li><i class="fa fa-bus"></i><a href="/attendances/index">출퇴근체크</a></li>
                            <li><i class="fa fa-list"></i><a href="/attendances/list">출퇴근기록</a></li>
                        </ul>
                    </li>

                    <% if(usertype == "admin") { %>
                        <h3 class="menu-title">admin</h3>
                        <% if(path1 == "admin") { %>
                        <li class="menu-item-has-children active dropdown show">
                        <% } else{ %>
                        <li class="menu-item-has-children dropdown">
                        <% } %>
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-users"></i>유저관리</a>
                            <ul class="sub-menu children activedropdown-menu show">
                                <li><i class="fa fa-address-card"></i><a href="/admin/userlist">유저리스트</a></li>
                            </ul>
                        </li>
                    <% } %>
                   
                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside><!-- /#left-panel -->

    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
        <header id="header" class="header">

            <div class="header-menu">

                <div class="col-sm-7">
                    <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                    <div class="header-left">
                        
                    </div>
                </div>

                <div class="col-sm-5">
                    <div class="user-area dropdown float-right">
                        <span style="line-height: 40px;"> <%= useremail %> </span>&nbsp;&nbsp;
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img class="user-avatar rounded-circle" src="images/default_user.png" alt="User Avatar">
                        </a>

                        <div class="user-menu dropdown-menu">
